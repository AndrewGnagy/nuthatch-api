          {
            "name": "order",
            "in": "query",
            "description": "Scientific order",
            "required": false,
            "type": "string"
          },
          {
            "name": "family",
            "in": "query",
            "description": "Scientific family",
            "required": false,
            "type": "string"
          },
          {
            "name": "status",
            "in": "query",
            "description": "Conservation status",
            "required": false,
            "type": "string"
          },
          {
            "name": "operator",
            "in": "query",
            "description": "operator to use",
            "required": false,
            "type": "enum",
            "enum": ["AND", "OR"],
            "default": "AND"
